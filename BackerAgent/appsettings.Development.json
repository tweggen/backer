{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HannibalServiceClient": {
    "BaseUrl": "http://localhost:5288",
    "no-BaseUrl": "https://api.essentialvault.de"
  },
  "IdentityApiServiceClient": {
    "BaseUrl": "http://localhost:5288",
    "no-BaseUrl": "https://api.essentialvault.de"
  },
  "RCloneService": {
    "RClonePath": "~\\bin\\rclone.exe",
    "RCloneOptions": "--log-level info --log-format json --rc-user \"who\" --rc-pass \"how\" rcd",
    "no-UrlSignalR": "https://api.essentialvault.de",
    "UrlSignalR": "http://localhost:5288"
  }
}
